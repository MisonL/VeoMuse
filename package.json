{
  "name": "veomuse",
  "version": "2.0.0",
  "description": "A web application for text-to-video and image-to-video generation using Gemini Veo model",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docker:build": "cd config/docker && docker build -f Dockerfile -t veomuse ../.. ",
    "docker:run": "cd config/docker && docker run -d -p 3000:3000 --env-file ../../.env veomuse",
    "docker:compose": "cd config/docker && docker-compose up -d",
    "pm2:start": "pm2 start config/pm2/ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop veomuse",
    "pm2:logs": "pm2 logs veomuse",
    "health:check": "curl -f http://localhost:3000/health || exit 1"
  },
  "keywords": [
    "video-generation",
    "gemini-veo",
    "ai",
    "text-to-video",
    "image-to-video"
  ],
  "author": "Developer",
  "license": "MIT",
  "homepage": "https://github.com/MisonL/VeoMuse#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MisonL/VeoMuse.git"
  },
  "bugs": {
    "url": "https://github.com/MisonL/VeoMuse/issues"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "axios": "^1.4.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "fluent-ffmpeg": "^2.1.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "redis": "^4.6.7",
    "socket.io": "^4.8.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "jest": "^29.5.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "server.js",
      "config.js"
    ]
  }
}
